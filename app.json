{
    "expo": {
        "name": "AB Capital",
        "slug": "ab-capital",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./app/assets/images/icon.png",
        "scheme": "myapp",
        "userInterfaceStyle": "light",
        "newArchEnabled": true,
        "splash": {
            "image": "./app/assets/images/splash-icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/images/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            }
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./src/app/assets/images/favicon.png"
        },
        "plugins": [
            [
                "expo-router",
                {
                    "root": "./src/pages"
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        }
    }
}
